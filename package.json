{
  "name": "karma-starwars-names",
  "version": "0.0.1",
  "description": "dummy package to learnpublishingnpm package",
  "main": "dist/index.js",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "npm-run-all --parallel build:*",
    "build:main": "babel --copy-files --out-dir dist src",
    "build:umd": "webpack --output-filename index.umd.js",
    "build:umd.min": "webpack --output-filename index.umd.min.js -p",
    "lint": "eslint src test",
    "test": "cross-env NODE_ENV=test nyc mocha",
    "watch:test": "mocha --watch",
    "report-coverage": "cat ./coverage/lcov.info | codecov",
    "validate": "npm-run-all --parallel test lint build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/karpagma/fem-npm-starwars-names.git"
  },
  "author": "",
  "files": ["dist"],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/karpagma/fem-npm-starwars-names/issues"
  },
  "homepage": "https://github.com/karpagma/fem-npm-starwars-names#readme",
  "dependencies": {
    "unique-random-array": "^1.0.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-istanbul": "^4.1.5",
    "babel-preset-es2015": "^6.24.1",
    "babel-register": "^6.26.0",
    "chai": "^4.1.2",
    "codecov": "^3.0.0",
    "cross-env": "^5.1.1",
    "eslint": "^4.11.0",
    "eslint-plugin-react": "^7.5.1",
    "ghooks": "^2.0.0",
    "json-loader": "^0.5.7",
    "mocha": "^4.0.1",
    "npm-run-all": "^4.1.2",
    "nyc": "^11.3.0",
    "rimraf": "^2.6.2",
    "webpack": "^3.8.1"
  },
  "nyc": {
    "check-coverage": true,
    "branches": 100,
    "functions": 100,
    "lines": 100,
    "statements": 100,
    "reporter": ["text", "lcov"],
    "instrument": false,
    "sourceMap": false,
    "require": ["babel-register"],
    "include": ["src"]
  },
  "babel": {
    "presets": ["es2015"],
    "env": {
      "test": {
        "plugins": ["istanbul"]
      }
    }
  },
  "config": {
    "ghooks": {
      "pre-commit": "npm run validate"
    }
  }
}
